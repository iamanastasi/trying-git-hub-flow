int min =0;
int max =0;
        int l=arr.length;
        for(int i=0;i<l;i++)
        {
            for(int j=0;j<l;j++)
            {
                if(j==0)
                {
                  min=arr[i][0];
                }
                if(arr[i][j]<min)
                {
                    min=arr[i][j];
                }
}
if(i==0)
{
max=min;
}
if (min>max)
{
max=min;
}
            }
            System.out.print(max);
